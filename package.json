{
  "name": "dataviz-nivo",
  "version": "1.0.0",
  "description": "Pokedex GraphQL API with Nivo",
  "repository": "https://github.com/israelias/dataviz-nivo",
  "author": "Joem Elias Sanez",
  "private": true,
  "scripts": {
    "dev": "yarn codegen && next dev",
    "build": "yarn codegen && next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml && prettier --write @types/* lib/* ",
    "next:lint": "next lint",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.graphql . --color",
    "format": "prettier --write './**/*.{ts,tsx,scss,css,json,graphql}'"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@nivo/core": "^0.73.0",
    "@nivo/radar": "^0.73.0",
    "@svgr/webpack": "^5.5.0",
    "cross-fetch": "^3.1.5",
    "framer-motion": "^4",
    "graphql": "^15.5.2",
    "graphql-codegen-apollo-next-ssr": "^1.7.4",
    "next": "15",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.2",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/plugin-helpers": "^3.1.2",
    "@graphql-codegen/typescript": "^2.2.0",
    "@graphql-codegen/typescript-operations": "^2.1.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^2.7.11",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "eslint": "^8.31.0",
    "eslint-config-next": "13.4.9",
    "graphql-let": "^0.18.4",
    "graphql-tag": "^2.12.6",
    "prettier": "3.4.2",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "typescript": "^4.9.4",
    "yaml-loader": "^0.8.0"
  },
  "engines": {
    "node": ">= 12.13 <18"
  }
}
